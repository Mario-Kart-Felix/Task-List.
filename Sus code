/*==============================
	 Suspend.java
==============================*/
package com.test.mvc;

public class Suspend
{
	// 주요 속성 구성
	private String id, acct_sus_code, acct_sus_date, acct_sus_date_end;

	private String suspend_code;
	
	// 추가 속성 구성
	private String ssn1, ssn2;	// 주민번호 앞자리, 뒷자리
	private int grade;			// 관리자0, 일반사원1
	
	public String getSuspend_code()
	{
		return suspend_code;
	}
	public void setSuspend_code(String suspend_code)
	{
		this.suspend_code = suspend_code;
	}
	public String getId()
	{
		return id;
	}
	public void setId(String id)
	{
		this.id = id;
	}
	public String getAcct_sus_code()
	{
		return acct_sus_code;
	}
	public void setAcct_sus_code(String acct_sus_code)
	{
		this.acct_sus_code = acct_sus_code;
	}
	public String getAcct_sus_date()
	{
		return acct_sus_date;
	}
	public void setAcct_sus_date(String acct_sus_date)
	{
		this.acct_sus_date = acct_sus_date;
	}
	public String getAcct_sus_date_end()
	{
		return acct_sus_date_end;
	}
	public void setAcct_sus_date_end(String acct_sus_date_end)
	{
		this.acct_sus_date_end = acct_sus_date_end;
	}
	public String getSsn1()
	{
		return ssn1;
	}
	public void setSsn1(String ssn1)
	{
		this.ssn1 = ssn1;
	}
	public String getSsn2()
	{
		return ssn2;
	}
	public void setSsn2(String ssn2)
	{
		this.ssn2 = ssn2;
	}
	public int getGrade()
	{
		return grade;
	}
	public void setGrade(int grade)
	{
		this.grade = grade;
	}
	
	
}
